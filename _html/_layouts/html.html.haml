!!! 5
%html{lang: "{{ page.language }}"}<>
  %head<>
    %meta{charset: "utf-8"}<>
    %title<> {% if page.language == 'zh' %}{{ site.title.zh }}{% else %}{{ site.title.en }}{% endif %}{% if page.title %}: {{ page.title }}{% endif %}
    %link{rel: "stylesheet", href: "//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css", type: "text/css"}<>
    %link{rel: "stylesheet", href: "//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css", type: "text/css"}<>
    %link{rel: "stylesheet", href: "//fonts.googleapis.com/css?family=Open+Sans:400,300,800,700", type: "text/css"}<>
    %link{rel: "stylesheet", href: "{{ site.static_url }}/{% if site.debug %}css/applysquare.css?{{ site.time | date: '%s' }}{% else %}{{ site.time | date: '%s' }}/css/applysquare.css{% endif %}", type: "text/css"}<>
    %link{rel: "alternate", href: "{{ site.feed.en }}", type: "application/atom+xml", title: "{{ site.title.en }}"}<>
    %link{rel: "alternate", href: "{{ site.feed.zh }}", type: "application/atom+xml", title: "{{ site.title.zh }}"}<>
    %link{rel: "canonical", href: "{{ site.url }}{{ page.url }}"}<>
    %meta{name: "description", content: "{% if page.subtitle %}{{ page.subtitle }}{% else %}{% if page.language == 'zh' %}{{ site.subtitle.zh }}{% else %}{{ site.subtitle.en }}{% endif %}{% endif %}"}<>
    %meta{name: "keywords", content: "{{ site.keywords }}"}<>
    %meta{name: "author", content: "{{ site.author.name }}"}<>
    %meta{name: "viewport", content: "user-scalable=0, initial-scale=1.0"}<>
  %body<>
    = "{{ content }}"
    = "{% include settings.html %}"
    %script{src: "//code.jquery.com/jquery-2.1.0.min.js", type: "text/javascript"}<>
    %script{src: "//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js", type: "text/javascript"}<>
    %script{src: "{{ site.static_url }}/{% if site.debug %}js/applysquare.js?{{ site.time | date: '%s' }}{% else %}{{ site.time | date: '%s' }}/js/applysquare.js{% endif %}", type: "text/javascript"}<>
